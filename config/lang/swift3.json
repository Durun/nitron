{
    "extensions": [".swift"],
	"grammarConfig": {
		"grammarFiles": [
			"../grammars/swift/swift3/Swift3.g4"
		],
		"utilJavaFiles": [
			"../grammars/swift/swift3/src/main/java/SwiftSupport.java"
		],
		"startRule": "top_level"
	},
	"processConfig": {
		"splitConfig": {
			"splitRules": [
				"declaration",
				"statement",
				"where_clause",
				"catch_clause",
				"if_directive_clause",
				"elseif_directive_clause",
				"else_directive_clause"
			]
		},
		"normalizeConfig": {
			"mapping": {
				"string_literal": "\"S\"",
				"numeric_literal": "N",
				"boolean_literal": "$L"
			},
			"indexedMapping": {
				"variable_name": "$V",
				"postfix_expression/primary_expression/declaration_identifier": "$V",
				"identifier_pattern/declaration_identifier": "$V",
				"function_call_argument/label_identifier": "$V"
			},
			"ignoreRules": [
				"import_declaration",
				"EOF"
			]
		}
	}
}
