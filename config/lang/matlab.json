{
    "extensions": [".m"],
	"grammarConfig": {
		"grammarFiles": [
			"../grammars/matlab/matlab.g4"
		],
		"utilJavaFiles": [],
		"startRule": "translation_unit"
	},
	"processConfig": {
		"splitConfig": {
			"splitRules": [
				"statement",
				"ELSE",
				"elseif_clause"
			]
		},
		"normalizeConfig": {
			"mapping": {
				"STRING_LITERAL": "'S'",
				"CONSTANT": "N"
			},
			"indexedMapping": {
				"postfix_expression/primary_expression/IDENTIFIER": "$V"
			},
			"nonNumberedRuleMap": [
				{
					"fromRules": [
						"STRING_LITERAL"
					],
					"toSymbol": "'S'"
				},
				{
					"fromRules": [
						"CONSTANT"
					],
					"toSymbol": "N"
				}
			],
			"numberedRuleMap": [
				{
					"fromRules": [
						"postfix_expression",
						"primary_expression",
						"IDENTIFIER"
					],
					"toSymbol": "$V"
				}
			],
			"ignoreRules": [
				"EOF"
			]
		}
	}
}
