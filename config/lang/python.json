{
	"extensions": [".py"],
	"parserConfig": {
	  "type": "antlr",
	  "grammarFiles": [
		"../grammars/python/python3/Python3Parser.g4",
		"../grammars/python/python3/Python3Lexer.g4"
	  ],
	  "utilJavaFiles": [
		"../grammars/python/python3/Java/Python3LexerBase.java"
	  ],
	  "startRule": "file_input"
	},
	"processConfig": {
		"splitConfig": {
			"splitRules": ["stmt"]
		},
		"normalizeConfig": {
			"mapping": {
				"STRING": "\"S\"",
				"NUMBER": "N",
				"INDENT": "INDENT",
				"DEDENT": "DEDENT"
			},
			"indexedMapping": {
				"atom/NAME": "$V"
			},
			"ignoreRules": [
				"import_stmt",
				"pass_stmt",
				"NEWLINE",
				"EOF"
			]
		}
	}
}
