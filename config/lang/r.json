{
  "extensions": [".r"],
  "grammarConfig": {
    "grammarFiles": [
      "../grammars/r/R.g4"
    ],
    "utilJavaFiles": [],
    "startRule": "prog"
  },
  "processConfig": {
    "splitConfig": {
      "splitRules": [
        "exprlist"
      ]
    },
    "normalizeConfig": {
      "mapping": {
        "STRING": "\"S\"",
        "HEX": "N",
        "INT": "N",
        "FLOAT": "N",
        "COMPLEX": "N",
        "'TRUE'": "$L",
        "'FALSE'": "$L"
      },
      "indexedMapping": {
        "ID": "$V"
      },
      "nonNumberedRuleMap": [
        {
          "fromRules": [
            "STRING"
          ],
          "toSymbol": "\"S\""
        },
        {
          "fromRules": [
            "HEX"
          ],
          "toSymbol": "N"
        },
        {
          "fromRules": ["INT"],
          "toSymbol": "N"
        },
        {
          "fromRules": ["FLOAT"],
          "toSymbol": "N"
        },
        {
          "fromRules": ["COMPLEX"],
          "toSymbol": "N"
        },
        {
          "fromRules": ["'TRUE'"],
          "toSymbol": "$L"
        },
        {
          "fromRules": ["'FALSE'"],
          "toSymbol": "$L"
        }
      ],
      "numberedRuleMap": [
        {
          "fromRules": ["ID"],
          "toSymbol": "$V"
        }
      ],
      "ignoreRules": [
        "EOF"
      ]
    }
  }
}
