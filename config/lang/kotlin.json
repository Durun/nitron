{
    "extensions": [".kt"],
	"grammarConfig": {
		"grammarFiles": [
			"../grammars/kotlin/kotlin/KotlinLexer.g4",
			"../grammars/kotlin/kotlin/KotlinParser.g4",
			"../grammars/kotlin/kotlin/UnicodeClasses.g4"
		],
		"utilJavaFiles": [],
		"startRule": "kotlinFile"
	},
	"processConfig": {
		"splitConfig": {
			"splitRules": ["statement"]
		},
		"normalizeConfig": {
			"nonNumberedRuleMap": [
				{
					"fromRules": ["stringLiteral"],
					"toSymbol": "\"S\""
				},
				{
					"fromRules": ["CharacterLiteral"],
					"toSymbol": "'C'"
				},
				{
					"fromRules": ["IntegerLiteral"],
					"toSymbol": "N"
				},
				{
					"fromRules": ["RealLiteral"],
					"toSymbol": "N"
				}
			],
			"numberedRuleMap": [
				{
					"fromRules": [
						"atomicExpression",
						"simpleIdentifier"
					],
					"toSymbol": "$V"
				},
				{
					"fromRules": [
						"variableDeclaration",
						"simpleIdentifier"
					],
					"toSymbol": "$V"
				}
			],
			"ignoreRules": [
				"annotation",
				"preamble"
			]
		}
	}
}
