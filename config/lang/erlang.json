{
    "extensions": [".erl"],
	"grammarConfig": {
		"grammarFiles": [
			"../grammars/erlang/Erlang.g4"
		],
		"utilJavaFiles": [],
		"startRule": "forms"
	},
	"processConfig": {
		"splitConfig": {
			"splitRules": [
				"form",
				"functionClause",
				"crClause"
			]
		},
		"normalizeConfig": {
			"mapping": {
				"tokString": "\"S\"",
				"tokChar": "'C'",
				"tokFloat": "N"
			},
			"indexedMapping": {
				"tokVar": "$V"
			},
			"nonNumberedRuleMap": [
				{
					"fromRules": [
						"tokString"
					],
					"toSymbol": "\"S\""
				},
				{
					"fromRules": [
						"tokChar"
					],
					"toSymbol": "'C'"
				},
				{
					"fromRules": ["tokFloat"],
					"toSymbol": "N"
				}
			],
			"numberedRuleMap": [
				{
					"fromRules": ["tokVar"],
					"toSymbol": "$V"
				}
			],
			"ignoreRules": [
				"EOF"
			]
		}
	}
}
