{
  "extensions": [
	".go"
  ],
  "parserConfig": {
	"type": "antlr",
	"grammarFiles": [
	  "../grammars/golang/GoParser.g4",
	  "../grammars/golang/GoLexer.g4"
	],
	"utilJavaFiles": [
	  "../grammars/golang/Java/GoParserBase.java"
	],
	"startRule": "sourceFile"
  },
  "processConfig": {
	"splitConfig": {
	  "splitRules": [
		"statement",
		"exprCaseClause"
	  ]
	},
	"normalizeConfig": {
	  "mapping": {
		"string_": "\"S\"",
		"RUNE_LIT": "'R'",
		"DECIMAL_LIT": "N",
		"BINARY_LIT": "N",
		"OCTAL_LIT": "N",
		"HEX_LIT": "N",
		"FLOAT_LIT": "F",
		"IMAGINARY_LIT": "I"
	  },
	  "indexedMapping": {
		"//declaration/varDecl/varSpec/identifierList/IDENTIFIER": "V",
		"//shortVarDecl/identifierList/IDENTIFIER": "V",
		"//primaryExpr[not(./following-sibling::*/@tag='arguments')]/operand/operandName/IDENTIFIER": "V"
	  },
	  "ignoreRules": [
		"packageClause",
		"importDecl",
		"//EOS[not(./text()=';')]",
		"EOF"
	  ]
	}
  }
}
